{
    "version": 2,
    "builds": [
        {
            "src": "https://cryptosphere.onrender.com",
            "use": "@vercel/java"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://cryptosphere.onrender.com",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
